<script>
$(function() {
if (Cookies.get('accessibility') =='on'){
		$("body").addClass('accessibility');
		$(".eye,.small-eye").addClass('on').attr("title", "Обычная версия сайта");
		$(".small-eye").find("span").text("Обычная версия сайта");
		$("#color").attr("href", "/local/templates/main/polytech-framework/css/color/polytech-accessible.css");
}
$('.eye,.small-eye').on('click', function(v){

	v.preventDefault();
	yaCounter26376306.reachGoal('visual-impared',{});
	if ( typeof Cookies.get('accessibility') === "undefined"){
			Cookies.set('accessibility', 'on', { expires: 3, path: '/' });
			$('body').addClass('accessibility');
			$("#color").attr("href", "/local/templates/main/polytech-framework/css/color/polytech-accessible.css");
			$('.eye,.small-eye').addClass('on').attr("title", "Обычная версия сайта");
			$(".small-eye").find("span").text("Обычная версия сайта");
		} else{
			$('body').removeClass('accessibility');
			$('.eye,.small-eye').removeClass('on');
			$('.eye,.small-eye').attr("title", "Версия для слабовидящих");
			$(".small-eye").find("span").text("Версия для слабовидящих");
			$("#color").attr("href", "/local/templates/main/polytech-framework/css/color/polytech-framework-color.css");
			Cookies.remove('accessibility', { path: '/' }); 
	}
});  
});</script>
